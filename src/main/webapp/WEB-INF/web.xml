<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuario</servlet-name>
        <servlet-class>Controlador.Usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reporte</servlet-name>
        <servlet-class>Controlador.Reporte</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Solicitudes</servlet-name>
        <servlet-class>Controlador.Solicitudes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaterialEleccion</servlet-name>
        <servlet-class>Controlador.MaterialEleccion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaterialEleccionProfesores</servlet-name>
        <servlet-class>Controlador.MaterialEleccionProfesores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LaboratorioS</servlet-name>
        <servlet-class>Controlador.LaboratorioS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Revision</servlet-name>
        <servlet-class>Controlador.Revision</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Material</servlet-name>
        <servlet-class>Controlador.Material</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuario</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reporte</servlet-name>
        <url-pattern>/Reporte</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Solicitudes</servlet-name>
        <url-pattern>/Solicitudes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaterialEleccion</servlet-name>
        <url-pattern>/MaterialEleccion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaterialEleccionProfesores</servlet-name>
        <url-pattern>/MaterialEleccionProfesores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LaboratorioS</servlet-name>
        <url-pattern>/LaboratorioS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Revision</servlet-name>
        <url-pattern>/Revision</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Material</servlet-name>
        <url-pattern>/Material</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
</welcome-file-list>

</web-app>
